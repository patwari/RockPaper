{
    "editor.codeLens": true,
    "editor.formatOnSave": true,
    "omnisharp.autoStart": true,
    "explorer.autoReveal": "focusNoScroll",
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.gitmodules": true,
        "**/*.booproj": true,
        "**/*.pidb": true,
        "**/*.suo": true,
        "**/*.pdf": true,
        "**/*.mid": true,
        "**/*.midi": true,
        "**/*.wav": true,
        "**/*.gif": true,
        "**/*.ico": true,
        "**/*.jpg": true,
        "**/*.jpeg": true,
        "**/*.png": true,
        "**/*.psd": true,
        "**/*.tga": true,
        "**/*.tif": true,
        "**/*.tiff": true,
        "**/*.3ds": true,
        "**/*.3DS": true,
        "**/*.fbx": true,
        "**/*.FBX": true,
        "**/*.lxo": true,
        "**/*.LXO": true,
        "**/*.ma": true,
        "**/*.MA": true,
        "**/*.obj": true,
        "**/*.OBJ": true,
        "**/*.cubemap": true,
        "**/*.flare": true,
        "**/*.mat": true,
        "**/*.meta": true,
        "build/": true,
        "Build/": true,
        "Library/": true,
        "library/": true,
        "obj/": true,
        "Obj/": true,
        "ProjectSettings/": false,
        "temp/": true,
        "Temp/": true
    },
    "cSpell.words": ["patwari"],
    "cSpell.language": "en,en-GB",
    "dotnet.defaultSolution": "RockPaper.sln",
    "dotnet.codeLens.enableReferencesCodeLens": false,
    "gitlens.codeLens.enabled": false,
    "prettier.useTabs": true,
    "prettier.tabWidth": 4,
    "prettier.printWidth": 150,
    "scm.autoReveal": true,
    "scm.compactFolders": true,
    "scm.diffDecorationsGutterPattern": {
        "added": true,
        "modified": true
    },
    "scm.diffDecorationsIgnoreTrimWhitespace": "inherit",
    "scm.graph.pageOnScroll": false,
    "scm.graph.pageSize": 20
}